{
    "name": "iankibet/redis-sub",
    "description": "A Laravel package to listen to Redis published messages.",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Iankibet\\RedisSub\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Iankibet\\RedisSub\\RedisSubServiceProvider"
            ]
        }
    }
}
